{
  "name": "sanctuary",
  "version": "4.14.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umbrella-network/sanctuary.git"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.ts ./test/**/*.ts --max-warnings=0",
    "lint:fix": "eslint ./src/**/*.ts ./test/**/*.ts --fix",
    "clean": "rimraf dist",
    "compile": "tsc -p .",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --check",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' 'test/**/*.ts' --write",
    "format": "npm run lint:fix && npm run prettier:format",
    "copy:assets": "cpx 'src/**/*.{graphql,html,png,json}' dist",
    "start:dev": "tsnd --respawn src/server.ts",
    "start:dev:worker": "tsnd --respawn src/worker.ts",
    "start:dev:scheduler": "tsnd --trace-warnings --respawn src/scheduler.ts",
    "task:dev": "tsnd --rs src/tasks.ts",
    "build": "npm run clean && npm run compile && npm run copy:assets",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "start:scheduler": "node dist/scheduler.js",
    "test": "cross-env NODE_ENV=testing MONGODB_URL='mongodb://localhost:27017/sanctuary-test' mocha --require ts-node/register --ui bdd 'test/**/*.test.ts'",
    "test:watch": "cross-env NODE_ENV=testing MONGODB_URL='mongodb://localhost:27017/sanctuary-test' mocha --watch --require ts-node/register --ui bdd 'test/**/*.test.ts'",
    "test:e2e": "cross-env NODE_ENV=testing MONGODB_URL='mongodb://localhost:27017/sanctuary-test' mocha --require ts-node/register --ui bdd 'test/**/*.e2e.ts'",
    "task": "node dist/tasks.js"
  },
  "devDependencies": {
    "@types/auth0": "~2.34.3",
    "@types/bcrypt": "~3.0.0",
    "@types/chai": "~4.2.14",
    "@types/chai-as-promised": "~7.1.3",
    "@types/compression": "~1.7.0",
    "@types/cookie-parser": "~1.4.2",
    "@types/cors": "~2.8.8",
    "@types/express": "~4.17.8",
    "@types/express-jwt": "^6.0.2",
    "@types/ioredis": "^4.27.0",
    "@types/js-yaml": "^4.0.1",
    "@types/jsonwebtoken": "~8.5.0",
    "@types/lodash": "^4.2.0",
    "@types/mocha": "~8.2.0",
    "@types/mongodb": "^3.5.28",
    "@types/moxios": "~0.4.9",
    "@types/newrelic": "~7.0.0",
    "@types/node": "~14.11.2",
    "@types/rosie": "^0.0.40",
    "@types/sinon": "^10.0.2",
    "@types/statsd-client": "^0.4.3",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "~8.3.0",
    "@types/yargs": "~15.0.8",
    "@typescript-eslint/eslint-plugin": "~4.9.0",
    "@typescript-eslint/parser": "~4.9.0",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "cross-env": "~7.0.3",
    "eslint": "~7.27.0",
    "mocha": "~8.2.1",
    "mock-jwks": "^1.0.1",
    "moxios": "~0.4.0",
    "nock": "^13.1.4",
    "prettier": "~2.2.1",
    "rosie": "^2.1.0",
    "sinon": "^11.1.2",
    "start-server-and-test": "~1.11.7",
    "supertest": "^6.1.6",
    "swagger-ui-express": "~4.1.6",
    "ts-mock-imports": "^1.3.7",
    "ts-mockito": "^2.6.1",
    "ts-node": "~9.0.0",
    "ts-node-dev": "~1.0.0-pre.63",
    "ts-sinon": "^2.0.2",
    "typescript": "~4.0.3"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.14.0",
    "@influxdata/influxdb-client-apis": "^1.14.0",
    "@project-serum/anchor": "~0.23.0",
    "@solana/web3.js": "~1.36.0",
    "@umb-network/toolbox": "~5.9.0",
    "auth0": "^2.37.0",
    "axios": "~0.21.1",
    "bcrypt": "~5.0.0",
    "bullmq": "^1.44",
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "cpx": "~1.5.0",
    "crypto-random-string": "~3.3.0",
    "date-fns": "^2.22.1",
    "dotenv": "~8.2.0",
    "ethers": "~5.5.1",
    "express": "~4.17.1",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^6.1.0",
    "fast-sort": "~3.0.3",
    "helmet": "~4.1.1",
    "inversify": "~5.0.1",
    "ioredis": "~4.17.3",
    "js-yaml": "4.0.0",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "~8.5.1",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "mongoose": "5.13.12",
    "newrelic": "~7.2.0",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.2",
    "statsd-client": "~0.4.5",
    "uuid": "~8.3.0",
    "uuidv4": "~6.2.4",
    "winston": "~3.3.3",
    "yargs": "~16.1.0"
  }
}
