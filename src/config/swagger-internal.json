              "application/json": {
                "schema": {
                  "type": "object",
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Register a new project",
        "tags": ["projects"],
        "security": [
          {
            "UserJWTToken": []
          }
        ],
        "requestBody": {
          "description": "A JSON containing the name of the project",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "name": {
                  "type": "string"
